<html lang="es">

<head>

    <title>Inicio Sesion | Web Plants</title>
    <!-- <link rel="icon" type="image/png" href="../login/images/Logo WebPlants.png" sizes="100x100"> -->
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.1.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
    <link rel="stylesheet" href="stylesheets/Home/styles-login.css">

</head>

<body class="no-touch" ontouchstart="">
    <div id="root">
        <main>
            <div
                class="MuiGrid-root jss5 MuiGrid-container MuiGrid-spacing-xs-1 MuiGrid-direction-xs-column MuiGrid-align-items-xs-center MuiGrid-justify-xs-center">
                <div class="MuiPaper-root MuiPaper-elevation1 jss127 MuiPaper-rounded" id="signInCard">
                    <a class="jss4" href="#" target="_blank">
                        <div class="logo jss112 jss7"><img class="jss113" src="images/Home/Logo WebPlants.png"
                                alt="Web Plants">
                        </div>
                    </a>
                    <h1 class="jss116" aria-label="Log in">Iniciar con</h1>
                    <div class="MuiGrid-root MuiGrid-container MuiGrid-spacing-xs-2 MuiGrid-justify-xs-space-around">
                        <div class="MuiGrid-root jss120 MuiGrid-item MuiGrid-grid-xs-4"><button
                                class="MuiButtonBase-root MuiButton-root jss158 MuiButton-contained" tabindex="0"
                                type="button" aria-label="Sign in with Google"><span class="MuiButton-label"><img
                                        class="jss159" src="images/Home/Google.png" alt=""><span
                                        class="jss160">Google</span></span><span
                                    class="MuiTouchRipple-root"></span></button></div>
                    </div>
                    <div class="jss183 jss182">or</div>
                    <div role="group" aria-label="Enter Credentials to sign in natively">
                        <form>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                <input type="text" class="form-control" name="username1" placeholder="email address">
                            </div>
                            <span class="help-block"></span>
                            <div class="jss238">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-lock"></i></span>
                                    <input type="password" class="form-control" name="password" placeholder="Password">
                                </div>
                                <button
                                    class="MuiButtonBase-root MuiButton-root jss239 MuiButton-text MuiButton-textPrimary"
                                    tabindex="0" type="button" id="showPasswordButton"><span
                                        class="MuiButton-label">show</span><span
                                        class="MuiTouchRipple-root"></span></button>
                            </div><a color="primary" class="jss241 jss129" href="#/forgotpassword">Se te olvidó tu
                                contraseña?</a>
                            <button
                                class="MuiButtonBase-root MuiButton-root jss122 MuiButton-contained MuiButton-containedPrimary"
                                tabindex="0" type="submit"><span class="MuiButton-label">Iniciar sesión</span><span
                                    class="MuiTouchRipple-root"></span>
                                <!-- <button class="btn btn-lg btn-primary btn-block" type="submit">Login</button> -->
                            </button>
                            <div class="jss123">No tienes una cuenta?<a class="jss241 jss124" color="primary"
                                    href="#/signup">Crea una cuenta</a></div>
                        </form>
                    </div>
                    <div></div>
                </div>
            </div>
        </main>
    </div>
